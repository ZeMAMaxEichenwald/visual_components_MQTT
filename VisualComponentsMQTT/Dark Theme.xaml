    <!--Control Content property -->
    <!-- more font choice http://www.microsoft.com/typography/fonts/family.aspx?FID=331-->
    <FontFamily x:Key="DefaultFont">Segoe UI</FontFamily>
    <FontFamily x:Key="FontFamilyTitle">Segoe UI Light</FontFamily>
    <FontFamily x:Key="DefaultFontFamily">SegoeUI Regular</FontFamily>

    <!--Strart : 13.5.2015 The new Font size key value-->
    <sys:Double x:Key="FontSizeNM4"   >8</sys:Double>
    <sys:Double x:Key="FontSizeNM3"   >9</sys:Double>
    <sys:Double x:Key="FontSizeNM2"   >10</sys:Double>
    <sys:Double x:Key="FontSizeNM1"   >11</sys:Double>
    <sys:Double x:Key="FontSizeN"     >12</sys:Double>
    <sys:Double x:Key="FontSizeNP1"   >13</sys:Double>
    <sys:Double x:Key="FontSizeNP2"   >14</sys:Double>
    <sys:Double x:Key="FontSizeNP3"   >15</sys:Double>
    <sys:Double x:Key="FontSizeNP4"   >16</sys:Double>
    <sys:Double x:Key="FontSizeNP5"   >17</sys:Double>
    <sys:Double x:Key="FontSizeNP6"   >18</sys:Double>
    <!--End-->

    <!--Control colors.-->
