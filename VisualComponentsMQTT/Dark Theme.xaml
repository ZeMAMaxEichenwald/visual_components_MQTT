    <Color x:Key="GridlineColor">#454545</Color>

    <SolidColorBrush opt:Freeze="True" x:Key="ThemeAlterBgFlipBrush"
                     Color="#F2F2F2" />

    <SolidColorBrush opt:Freeze="True" x:Key="ThemeAlterFgFlipBrush"
