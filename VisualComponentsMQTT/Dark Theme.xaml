    <SolidColorBrush opt:Freeze="True" x:Key="ThemeIdM1Brush" Color="{StaticResource ThemeIdM1Color}"/>
    <SolidColorBrush opt:Freeze="True" x:Key="ThemeIdBrush"   Color="{StaticResource ThemeIdColor  }" />
