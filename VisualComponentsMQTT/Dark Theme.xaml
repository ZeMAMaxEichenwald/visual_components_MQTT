    <Color x:Key="ThemeIdColor"  >#31758F</Color>
    <Color x:Key="ThemeIdP1Color">#99CBE3</Color>
    <Color x:Key="ThemeIdP2Color">#CDE6F7</Color>

    <Color x:Key="ThemeFgM2Color">#666666</Color>
    <Color x:Key="ThemeFgM1Color">#999999</Color>
    <Color x:Key="ThemeFgColor"  >#eaeaea</Color>
    <Color x:Key="ThemeFgP1Color">#f2f2f2</Color>
    <Color x:Key="ThemeFgP2Color">#ffffff</Color>
    <Color x:Key="ThemeFgP3Color">#ffffff</Color>

    <Color x:Key="ThemeMarkFgColor">#ffffff</Color>
